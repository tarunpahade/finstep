{
  "expo": {
    "name": "Finstep",
    "slug": "finstep2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/finstep.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mobile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ],
        [
          "expo-camera",
          {
            "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tarun.finstep",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/finstep.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.finsteps.finstep",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2ad8fcd6-44ba-4ee7-bfdd-b0672e46ddf4"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/aaedae10-bd91-48a0-ad30-8df5796d07e2"
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
